public class Reservation extends Booking {

public boolean makeReservation(int userId, LocalDate reservationTime, int numberOfCustomers, ){
		Table table = null;

		Reservation Reservation = new Reservation(customerId, table.tableId, reservationTime, numberOfCustomers);
		
		

class Reservation{
	int reservationId;
	int customerId;
	int tableId;
	LocalDate creationTime;
	LocalDate reservationTime;
	int numberOfCustomers
	
	public Reservation(int customerId, int tableId, LocalDate reservationTime, int numberOfCustomers){
		this.reservationId = reservationId;
		this.tableId = tableId;
		this.customerId = customerId;
		this.reservationTime = reservationTime;
		this.numberOfCustomers = numberOfCustomers;
	}
	

	
}

	}

	
	}
