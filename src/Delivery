public class Delivery extends Orders {

    public int customerID;
    public Datetime date;
    public int DeliveryID;
    public String array orderList;
    public [] MenuItems;
    public int employeeID;

    public makeDelivery(int customerID, int employeeID, Date date, [] MenuItems ) {
        this.userID = userID;
        this.date = date;
        this.MenuItems=MenuItems;
        this.employeeId=employeeID
        Delivery Delivery = new Delivery(int customerID, int employeeID, Date date, [] MenuItems);
}

    // The methods:

    public int get_DeliveryID() { return DeliveryID; }

    public String get_date() { return date; }

    public String get_userID() { return  userID;}
    
    public Array get_MenuItems(){ return MenuItems;}
}
