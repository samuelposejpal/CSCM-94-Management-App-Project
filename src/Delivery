public class Delivery extends Orders {

    public int userID;
    private Date date;
    public int DeliveryID;
    public String array orderList;

    public makeDelivery(int userID, Date date ) {
        this.userID = userID;
        this.date = date;
    
        Delivery Delivery = new Delivery(userID,date);
}

    // The methods:

    public int get_DeliveryID() { return DeliveryID; }

    public String get_date() { return date; }

    public String get_userID() { return  userID;}
}
